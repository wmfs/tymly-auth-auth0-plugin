{
  "Comment": "Lists the Auth0 groups to RBAC roles mappings",
  "Version": "1.0",
  "version": "1.0",

  "StartAt": "ListMappings",
  "States": {
    "ListMappings": {
      "Type": "Task",
      "Resource": "module:listAuth0Mappings",
      "ResultPath": "$.mappings",
      "End": true
    }
  },

  "restrictions": [
    {
      "roleId": "tymly_rbacAdmin",
      "allows": [
        "*"
      ]
    }
  ]
}
{
  "Comment": "Remove an Auth0 Group to RBAC role mapping",
  "Version": "1.0",
  "version": "1.0",

  "StartAt": "RemoveMapping",
  "States": {
    "RemoveMapping": {
      "Type": "Task",
      "Resource": "module:removeAuth0Mapping",
      "Parameters": {
        "auth0.$": "$.auth0",
        "roleId.$": "$.roleId"
      },
      "ResultPath": null,
      "End": true
    }
  },

  "restrictions": [
    {
      "roleId": "tymly_rbacAdmin",
      "allows": [
        "*"
      ]
    }
  ]
}
